{
  "entrypoint": "index.html",
  "shell": "src/main/shell-app.js",
  "sources": [
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "design/**/*",
    "src/main/ui/**/*",
    "src/main/ui/ui-root.js",
    "node_modules/@webcomponents/webcomponentsjs/**/*",
    "node_modules/@webcomponents/shadycss/**/*",
    "src/components/**/*",
    "src/main/routing/**/*",
    "src/main/ui/**/*",
    "push-manifest.json"
  ],
  "fragments": [
    "src/pages/bacon-page.js",
    "src/pages/grid-page.js"
  ],
  "builds": [{
    "name": "esm-bundled",
    "browserCapabilities": [
      "es2015",
      "serviceworker",
      "modules"
    ],
    "js": {
      "compile": true,
      "minify": true,
      "transformModulesToAmd": true
    },
    "css": {
      "minify": true
    },
    "html": {
      "minify": true
    },
      "bundle": true,
    "bundle": true,
    "addServiceWorker": true
  }],
  "moduleResolution": "node",
  "npm": true
}